# Environment Variables for Orchestration Tools Comparison
# Copy this file to .env and fill in your values

# API Keys
GEMINI_API_KEY=your_gemini_api_key_here
OPENAI_API_KEY=your_openai_api_key_here

# Orchestration Tool Credentials
# Airflow
AIRFLOW_USERNAME=airflow
AIRFLOW_PASSWORD=airflow

# Dagster
DAGSTER_HOME=/path/to/dagster/home

# Kubeflow
KUBEFLOW_ENDPOINT=http://localhost:8080
KUBEFLOW_USERNAME=user
KUBEFLOW_PASSWORD=password

# Mage
MAGE_API_KEY=your_mage_api_key_here

# Database Connections
DB_HOST=localhost
DB_PORT=5432
DB_USER=user
DB_PASSWORD=password
DB_NAME=nutrition_rag

# Other Settings
LOG_DIRECTORY=./logs
CACHE_DIRECTORY=./cache 